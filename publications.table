 author = {Johannes Schmid and Frederik Beutler and Benjamin Noack and Uwe D. Hanebeck and Klaus D. Müller-Glaser},
 booktitle = {Proceedings of the 8th European Conference on Wireless Sensor Networks (EWSN 2011)},
